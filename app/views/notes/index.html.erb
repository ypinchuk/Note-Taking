<h1>Listing Notes</h1>
<%= link_to 'New Note', new_note_path %>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @notes.each do |note| %>
    <tr>
      <td><%= note.title %></td>
      <td><%= note.text %></td>
      <td><%= link_to 'Show', note_path(note) %></td>
    </tr>
  <% end %>
</table>